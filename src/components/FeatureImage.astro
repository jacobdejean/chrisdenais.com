---
const { title, links } = Astro.props
---

<section>
	<div class='container extra-pb'>
		<p>{title}</p>
		{
			links &&
				links.map(({ href, label }) => (
					<a
						class='h1 link underscore-offset-2 icon'
						target='_blank'
						href={href}
					>
						{label}
					</a>
				))
		}
	</div>
</section>
